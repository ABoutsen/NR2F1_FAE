library(Seurat)

seurat_OBfree <- readRDS("../../seurat_OBfree.rds")

FeaturePlot(seurat_OBfree, features = c("Neurod1", 'Pcp4', 'Sema3c', 'Unc5d'))

FeaturePlot(seurat_OBfree, features = c("Neurod1"))
FeaturePlot(seurat_OBfree, features = c('Pcp4'))
FeaturePlot(seurat_OBfree, features = c('Unc5d'))
FeaturePlot(seurat_OBfree, features = c('Sema3c'))
